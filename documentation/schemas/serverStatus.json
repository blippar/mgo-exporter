                },
                "user": {
                    "type": "integer"
                },
                "warning": {
                    "type": "integer"
                }
            }
        },
        "connections": {
            "type": "object",
            "properties": {
                "available": {
                    "type": "integer"
                },
                "current": {
                    "type": "integer"
                },
                "totalCreated": {
                    "type": "integer"
                }
            }
        },
        "error": {
            "type": "string"
        },
        "globalLock": {
            "type": "object",
            "properties": {
                "activeClients": {
                    "type": "object",
                    "properties": {
                        "readers": {
                            "type": "integer"
                        },
                        "total": {
                            "type": "integer"
                        },
                        "writers": {
                            "type": "integer"
                        }
                    }
                },
                "currentQueue": {
                    "type": "object",
                    "properties": {
                        "readers": {
                            "type": "integer"
                        },
                        "total": {
                            "type": "integer"
                        },
                        "writers": {
                            "type": "integer"
                        }
                    }
