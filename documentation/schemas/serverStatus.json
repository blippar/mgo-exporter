        "opcountersRepl": {
            "type": "object",
            "properties": {
                "command": {
                    "type": "integer"
                },
                "delete": {
                    "type": "integer"
                },
                "getmore": {
                    "type": "integer"
                },
                "insert": {
                    "type": "integer"
                },
                "query": {
                    "type": "integer"
                },
                "update": {
                    "type": "integer"
                }
            }
