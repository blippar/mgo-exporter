            "type": "string"
        },
        "localTime": {
            "type": "string",
            "format": "date-time"
        },
        "mem": {
            "type": "object",
            "properties": {
                "bits": {
                    "type": "integer"
                },
                "mapped": {
                    "type": "integer"
                },
                "mappedWithJournal": {
                    "type": "integer"
                },
                "resident": {
                    "type": "integer"
                },
                "supported": {
                    "type": "integer"
                },
                "virtual": {
                    "type": "integer"
                }
            }
