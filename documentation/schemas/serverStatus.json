        "pid": {
            "type": "integer"
        },
        "process": {
            "type": "string"
        },
        "uptime": {
            "type": "integer"
        },
        "uptimeEstimate": {
            "type": "integer"
        },
        "uptimeMillis": {
            "type": "integer"
        },
        "version": {
            "type": "string"
        }
